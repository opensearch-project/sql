/*
{
  "name": "asc_sort_timestamp_no_can_match_shortcut",
  "operation-type": "search",
  "index": "{{index_name | default('big5')}}",
  "request-params" : {
      "pre_filter_shard_size" : 100000
  },
  "body": {
    "track_total_hits": false,
    "query": {
      "match": {
        "process.name": "kernel"
      }
    },
    "sort" : [
      {"@timestamp" : "asc"}
    ]
  }
}
*/
source = big5 process.name=kernel
| sort + `@timestamp`
| head 10