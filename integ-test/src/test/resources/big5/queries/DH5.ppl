source = big5 | eval range_group = case(`metrics.size` < -10, "< -10", `metrics.size` >= -10 and `metrics.size` < 10, "-10 to 10", `metrics.size` >= 10 and `metrics.size` < 100, "10 to 100", `metrics.size` >= 100 and `metrics.size` < 1000, "100 to 1000", `metrics.size` >= 1000 and `metrics.size` < 2000, "1000 to 2000", `metrics.size` >= 2000, ">= 2000") | stats count(), min(`metrics.tmin`) as min_temp, avg(`metrics.size`) as avg_size,  max(`metrics.size`) as max_size by range_group, span(@timestamp, 1h)