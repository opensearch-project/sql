/*
{
  "name": "desc_sort_timestamp_can_match_shortcut",
  "operation-type": "search",
  "index": "{{index_name | default('big5')}}",
  "body": {
    "track_total_hits": false,
    "query": {
      "match": {
        "process.name": "kernel"
      }
    },
    "sort" : [
      {"@timestamp" : "desc"}
    ]
  }
}
*/
source = big5 process.name=kernel
| sort - `@timestamp`
| head 10