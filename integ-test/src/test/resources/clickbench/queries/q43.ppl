// EventDate >= '2013-07-01' should work in v3
source=hits
| where EventDate >= '2013-07-01 00:00:00' and EventDate <= '2013-07-15 00:00:00' and CounterID = 62 and IsRefresh = 0 and DontCountHits = 0
| eval M = date_trunc('minute', EventTime)
| stats count() as PageViews by M
| sort M
| head 10 from 1000